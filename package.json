{
  "name": "metademo",
  "private": true,
  "version": "0.0.1",
  "description": "Metabase Demo",
  "repository": "https://github.com/metabase/embedding-demo",
  "license": "private",
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "export": "next build && next export",
    "start": "next start -p 3003",
    "start:ci": "NODE_ENV=test next build && NODE_ENV=test next start -p 3003",
    "type": "tsc --noEmit",
    "lint": "next lint",
    "lint:reports": "eslint --rulesdir ../lint/eslint-rules '**/*.{js,jsx,ts,tsx,json}'",
    "test-formatting": "prettier --check --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json}'",
    "update-formatting": "prettier --write --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json}'",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "checks": "concurrently npm:lint npm:type npm:test-formatting npm:test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11",
    "styled-media-query": "^2.1.2",
    "styled-system": "^5.1.5",
    "url": "^0.11.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.1.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fetch-mock-jest": "^1.5.1",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "next-router-mock": "^0.9.10",
    "node-mocks-http": "^1.12.2",
    "prettier": "^2.8.8",
    "transit-js": "^0.8.874",
    "typescript": "^5.0.4",
    "webpack": "^5.85.0",
    "webpack-dev-server": "^4.15.0",
    "whatwg-fetch": "^3.6.2",
    "yarn-upgrade-all": "^0.7.2"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.16",
    "node": ">=14.6.0"
  },
  "browserslist": [
    "> 1%",
    "not dead"
  ]
}
